<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x214"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    xmlns:circleimageView="http://schemas.android.com/apk/res-auto">


    <RelativeLayout
        android:id="@+id/head_rool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <com.emiaoqian.express.views.CircleImageView
            android:layout_marginLeft="@dimen/x80"
            android:layout_marginRight="@dimen/x80"
            android:layout_marginTop="@dimen/y31"
            android:id="@+id/headIcon"
            android:layout_width="@dimen/x54"
            android:src="@drawable/ic_launcher"
            android:layout_height="@dimen/x54"
            android:layout_gravity="center"
            android:layout_marginBottom="8dp"
            circleimageView:border_color="#ffffff"
            circleimageView:border_width="1dp" />


        <TextView
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/x85"
            android:textColor="@color/text_color"
            android:textSize="18sp"
            android:layout_below="@+id/headIcon"
            android:text="姓名（空）"
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



        <TextView
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/x2"
            android:layout_marginTop="@dimen/y2"
            android:drawablePadding="2dp"
            android:textSize="12sp"
            android:layout_below="@+id/tv_name"
            android:textColor="@color/text_color"
            android:text="17600176000"
            android:id="@+id/start_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


        <!-- android:src="@drawable/level_im"-->
        <ImageView
            android:id="@+id/levelIm"
            android:layout_marginTop="@dimen/x2"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/start_tv"
            android:layout_width="@dimen/x45"
            android:layout_height="@dimen/x7" />


    </RelativeLayout>


   <ListView
       android:divider="@null"
       android:id="@+id/lv_menu"
       android:layout_marginTop="@dimen/x34"
       android:layout_below="@+id/head_rool"
       android:layout_width="match_parent"
       android:layout_height="300dp"/>




    <LinearLayout
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
        android:layout_marginLeft="@dimen/x10"
            android:id="@+id/present_rl"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView
              android:layout_centerHorizontal="true"
                android:id="@+id/present_im"
                android:src="@drawable/present"
                android:layout_width="@dimen/x32"
                android:layout_height="@dimen/x32" />

            <TextView
               android:layout_centerHorizontal="true"
                android:layout_below="@+id/present_im"
                android:text="推荐有奖"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/x15" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginBottom="@dimen/x10"
            android:layout_alignParentBottom="true"
            android:id="@+id/job_rl"
            android:layout_toRightOf="@+id/present_rl"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_centerHorizontal="true"
                android:id="@+id/job_im"
                android:src="@drawable/jobforpeople"
                android:layout_width="@dimen/x32"
                android:layout_height="@dimen/x32" />

            <TextView
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/job_im"
                android:text="小哥招募"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/x15" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginRight="@dimen/x10"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/x10"
            android:layout_alignParentBottom="true"
            android:id="@+id/package_rl"
            android:layout_toRightOf="@+id/job_rl"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_centerHorizontal="true"
                android:id="@+id/package_im"
                android:src="@drawable/my_card"
                android:layout_width="@dimen/x32"
                android:layout_height="@dimen/x32" />

            <TextView
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/package_im"
                android:text="我的名片"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/x15" />
        </RelativeLayout>




    </LinearLayout>

